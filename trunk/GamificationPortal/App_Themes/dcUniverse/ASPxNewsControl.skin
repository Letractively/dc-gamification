<%@ Register TagPrefix="dx" Namespace="DevExpress.Data" Assembly="DevExpress.Data.v12.2, Version=12.2.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>
<%@ Register TagPrefix="dx" Namespace="DevExpress.Web.ASPxNewsControl" Assembly="DevExpress.Web.v12.2, Version=12.2.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>

<dx:ASPxNewsControl runat="server" SpriteCssFilePath="{0}/sprite.css" CssPostfix="dcUniverse" CssFilePath="{0}/styles.css">
    <ItemSettings DateVerticalPosition="Header" DateHorizontalPosition="Right" TailPosition="NewLine">
        <TailImage Url="Web/ncTail.png" />
    </ItemSettings>
    <BackToTopImage Url="Web/ncGotoBack.png" />
    <PagerSettings CurrentPageNumberFormat="{0}">
        <PageSizeItemSettings ShowPopupShadow="False">
            <DropDownImage>
                <SpriteProperties HottrackedCssClass="dxWeb_pPopOutHover_dcUniverse" PressedCssClass="dxWeb_pPopOutPressed_dcUniverse" />
            </DropDownImage>
        </PageSizeItemSettings>
    </PagerSettings>
    <LoadingPanelImage Url="Web/Loading.gif" />
    <PagerStyle ItemSpacing="0px" Spacing="0px" />
</dx:ASPxNewsControl>

